#tag Events Canvas1
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.DrawPicture(ShortsDesignerIcon,0,0,g.Width,g.Height,0,0,ShortsDesignerIcon.Width,ShortsDesignerIcon.Height)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events lblVersion
	#tag Event
		Sub Open()
		  if app.StageCode = app.Alpha then
		    
		    me.Text = "Alpha " + app.ShortVersion + " (Build " + str(app.NonReleaseVersion) + ")"
		    
		  elseif app.StageCode = app.Beta then
		    
		    me.Text = "Beta " + app.ShortVersion + " (Build " + str(app.NonReleaseVersion) + ")"
		    
		  else
		    
		    me.Text = "Version " + app.ShortVersion + " (Build " + str(app.NonReleaseVersion) + ")"
		    
		  end
		End Sub
	#tag EndEvent
#tag EndEvents
