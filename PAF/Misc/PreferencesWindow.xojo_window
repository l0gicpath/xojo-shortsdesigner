#tag Events pmFooterDate
	#tag Event
		Sub Open()
		  Dim base As new MenuItem
		  
		  BuildDateTimeMenu(base)
		  
		  For i as integer = 0 to base.Count - 1
		    
		    me.AddRow base.Item(i).Text
		    me.RowTag(me.ListCount-1) = base.Item(i).Tag
		    
		  next
		  
		  Dim sFormat as String = GetPref("DateFormat")
		  
		  if sFormat <> "" then
		    
		    for i as integer = 0 to me.ListCount-1
		      if me.RowTag(i) = sFormat then
		        me.ListIndex = i
		        exit
		      end
		    next
		    
		  end if
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Change()
		  SetPref("DateFormat","String",me.RowTag(me.ListIndex))
		  
		  WritePrefs
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbOK
	#tag Event
		Sub Action()
		  self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbBounds
	#tag Event
		Sub Action()
		  Dim frontmostDocumentWindow As Window
		  Dim lastOffset As Integer = WindowCount - 1
		  For i As Integer = 0 To lastOffset
		    Dim w As Window = Window(i)
		    If (w <> Nil) And (w.Frame = 0) And w.Visible Then
		      frontmostDocumentWindow = w
		      Exit
		    End If
		  Next
		  
		  if frontmostDocumentWindow <> nil then
		    
		    SetPref("winLeft","Integer",frontmostDocumentWindow.Bounds.Left)
		    SetPref("winTop","Integer",frontmostDocumentWindow.Bounds.Top)
		    SetPref("winWidth","Integer",frontmostDocumentWindow.Bounds.Width)
		    SetPref("winHeight","Integer",frontmostDocumentWindow.Bounds.Height)
		    
		    WritePrefs
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
